    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch + simulated environment",            
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=use_simulated_environment=true"
            ],
        },
        {
            "name": "Launch debug",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "Launch profile",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "Launch release",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}